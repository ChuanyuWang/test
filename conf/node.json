{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es2017": true
    },
    "rules": {
        "no-console": "off",
        "no-redeclare": "off",
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "args": "none",
                "ignoreRestSiblings": false
            }
        ],
        "no-prototype-builtins": "off"
    }
}