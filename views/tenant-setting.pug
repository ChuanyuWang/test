tenant-setting(style='display:none' feature=tenant_feature)
script.
    function _getTenantConfig() {
        return {
            logoPath: "#{logoPath}",
            contact: "#{tenant_contact}",
            address: "#{tenant_address}",
            addressLink: "#{tenant_addressLink}",
            classrooms: !{JSON.stringify(classrooms)},
            types: !{JSON.stringify(types)},
            feature: "#{tenant_feature}"
        };
    };

if tenant_systemMessage
    div.alert.alert-danger.system-warning-message
        button.close(type="button" data-dismiss="alert") &times;
        span.glyphicon.glyphicon-exclamation-sign
        = tenant_systemMessage
