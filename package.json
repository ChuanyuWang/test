{
  "name": "e-education",
  "version": "0.9.0",
  "description": "On-line course management",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --ext .js,.vue",
    "build": "webpack --config webpack.config.prod.js",
    "bower": "bower",
    "start": "node ./bin/www",
    "mongo-express": "env ME_CONFIG_MONGODB_ENABLE_ADMIN=true ME_CONFIG_MONGODB_ADMINUSERNAME=admin ME_CONFIG_MONGODB_ADMINPASSWORD=pass mongo-express"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.7",
    "bower": "^1.8.8",
    "compression": "^1.7.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "crypto-js": "^3.1.9-1",
    "express": "~4.16.4",
    "express-rate-limit": "^2.11.0",
    "express-session": "~1.15.6",
    "helmet": "^3.12.0",
    "i18n": "^0.8.3",
    "log4js": "^6.3.0",
    "mongoist": "^2.0.0",
    "mongojs": "~2.6.0",
    "mongoose": "^4.13.7",
    "monk": "^6.0.5",
    "morgan": "^1.10.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "wechat": "^2.0.3",
    "wechat-api": "^1.27.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-webpack-plugin": "^2.1.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "nodemon": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "optionalDependencies": {
    "mongo-express": "^0.42.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChuanyuWang/test.git"
  },
  "keywords": [
    "nodejs",
    "express"
  ],
  "author": "Chuanyu Wang",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ChuanyuWang/test/issues"
  },
  "homepage": "https://github.com/ChuanyuWang/test#readme",
  "nodemonConfig": {
    "exec": "npm start",
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "routes/",
      "locales/",
      "./*.js"
    ],
    "env": {
      "NODE_ENV": "development",
      "HOTRELOAD": true
    },
    "delay": "500"
  }
}
