{
  "name": "e-education",
  "version": "1.1.0",
  "description": "On-line course management",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --ext .js,.vue",
    "build": "vite build",
    "start": "node ./bin/www",
    "test": "mocha --require ./test/hooks.cjs --recursive",
    "worker": "node ./bin/worker"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.7",
    "bent": "^7.3.12",
    "body-parser": "^1.19.2",
    "body-parser-xml": "^2.0.3",
    "compression": "^1.7.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^3.1.9-1",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "helmet": "^4.2.0",
    "i18n": "^0.14.2",
    "log4js": "^6.4.2",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "mongodb": "^3.7.4",
    "mongojs": "^3.1.0",
    "mongoose": "^5.13.21",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local-mongoose": "^6.3.0",
    "pug": "^3.0.3",
    "serve-favicon": "^2.5.0",
    "wechat": "^2.0.3",
    "wechat-api": "^1.27.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.3.3",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-plugin-compat": "~4.2.0",
    "eslint-plugin-vue": "^9.27.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "less": "^3.12.2",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.3",
    "vite": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChuanyuWang/test.git"
  },
  "keywords": [
    "nodejs",
    "express"
  ],
  "author": "Chuanyu Wang",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ChuanyuWang/test/issues"
  },
  "homepage": "https://github.com/ChuanyuWang/test#readme",
  "nodemonConfig": {
    "exec": "npm start",
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "routes/",
      "locales/",
      "server/",
      "./*.js"
    ],
    "env": {
      "NODE_ENV": "development",
      "HOTRELOAD": true
    },
    "delay": "500"
  }
}